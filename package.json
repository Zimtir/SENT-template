{
  "name": "sent-template",
  "description": "Svelte Express Node Typescript",
  "version": "0.0.5",
  "scripts": {
    "build": "sapper build --legacy",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "dev": "sapper dev",
    "export": "sapper export --legacy",
    "eslint": "eslint --cache --ext .js,.ts, src",
    "eslint:dump": "eslint --print-config ./.eslintrc.json",
    "eslint:fix": "eslint --fix ./*.js ./src/*.ts ./src/**/*.ts",
    "format-and-fix": "npm run prettier && npm run eslint --fix",
    "prettier": "npm run prettier:typescript && npm run prettier:javascript && npm run prettier:svelte",
    "prettier:typescript": "prettier --write 'src/**/*.ts'",
    "prettier:javascript": "prettier --write 'src/**/*.js'",
    "prettier:svelte": "prettier --write 'src/**/*.ts'",
    "start": "node __sapper__/build",
    "test": "run-p --race dev cy:run",
    "update:install": "npm update && npm install",
    "update:pull": "npm outdated"
  },
  "dependencies": {
    "node-crud-kit": "^1.4.0",
    "sirv": "^0.4.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.4",
    "@rollup/plugin-commonjs": "~11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-typescript": "^5.0.1",
    "@types/node": "^13.11.0",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "rollup": "^1.20.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "sapper": "^0.27.0",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^4.0.6",
    "svelte-ts-preprocess": "^1.1.3",
    "ts-node": "^8.6.2"
  }
}
